<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Audit</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/Main.css" rel="stylesheet" />

  
    <script type="text/javascript">
        function navigate() {
            // Open in new window
            location.href = 'http://ladotnet';
        }

        $(document).ready(function () {
            $('.vertical-nav li a').mouseover(function () {
                $(this).parent().find('.sub-menu').show();
            })
                .mouseout(function () {
                    $(this).parent().find('.sub-menu').hide();
                });
        });

        function search() {           
            var input = document.getElementById('input_name').value;
            if (input == "Phone and Intranet") { return false; }
            if (input == "I am  looking For...") { return false; }
            window.location.href = "http://ladotd:3002/phon0200?input_name=" + input;
        }

        $(document).ready(function () {

            $("#input_name").on("keydown", function (e) {
                if (e.keyCode === 13) {  //checks whether the pressed key is "Enter"
                    search();
                }
            });

        });

    </script>

</head>
<body>
    <div id="goog-gt-tt" class="skiptranslate" dir="ltr">
            <div style="padding: 8px;">
                <div>
                    <div class="logo">
                        <img src="https://www.gstatic.com/images/branding/product/1x/translate_24dp.png" width="20" height="20" alt="Google Translate">
                    </div>
                </div>
            </div><div class="top" style="padding:8px; float: left; width: 100%;">
                <h1 class="title gray">Original text</h1>
            </div><div class="middle" style="padding: 8px;">
                <div class="original-text"></div>
            </div><div class="bottom" style="padding: 8px;">
                <div class="activity-links">
                    <span class="activity-link">Contribute a better translation</span><span class="activity-link"></span>
                </div>
                <div class="started-activity-container">
                    <hr style="color: #CCC; background-color: #CCC; height: 1px; border: none;">
                    <div class="activity-root"></div>
                </div>
            </div><div class="status-message" style="display: none;"></div>
        </div>
    <div id="page-container">
        <div align="center" id="content-wrap">
            <div class="noprint">
                <div style="height: 20px">
                    <div id="TopBar">
                        <div class="txt_format_1 float_left">DEPARTMENT OF TRANSPORTATION AND DEVELOPMENT</div>
                        <div><a href="http://wwwsp.dotd.la.gov" class="linkstyle_green" title="Internet">Internet</a></div>
                        <div><a href="http://apps1/administration/announcements/" class="linkstyle_green" title="MyDOTD">MyDOTD</a></div>
                        <div><a href="http://dotdport/" class="linkstyle_green" title="SharePoint">SharePoint</a></div>
                        <div><a href="http://apps1/administration/dotd_az/" class="linkstyle_green" title="DOTD AZ">DOTD AZ</a></div>
                        <div><a href="http://dotdport/applications/cs/SitePages/Home.aspx" class="linkstyle_green" title="Customer Service">Customer Service</a></div>
                    </div>
                </div>
            </div>
            <div class="noprint">
                <div style="height:51px">
                    <div>
                        <div id="GfxBar_Wide" onclick="navigate();">
                            <div id="GfxBar_Left"></div>
                            <div id="GfxBar_Right" style="z-index:-1">
                            </div>
                        </div>
                        <div id="topnav">
                            <div class="SearchPromptText">Can't find it? Search for it!</div>
                            <div class="searchPromptBox">
                                <div id="pnlSearch">
                                    <input name="name" type="text" value="I am looking for..." id="input_name" onfocus="$(this).mouseup(function() {
                                          if(this.value=='I am looking for...')this.value='';
                                          $(this).select();
                                         });" style="z-index:10;width:200px;">

                                    <input type="submit" name="btnPHONESearch" value="Search" id="btnPHONESearch" onclick=search()>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="noprint">
                <div class="grey_spacer"></div><div class="grey_spacer1"></div>
            </div>
            <div name="ToolBar_noSearch" class="noprint">
                <div colspan="2" style="height:24px; vertical-align: middle;">
                    <div id="ToolBar" style="display: block;">
                        <span id="BreadCrumbs" class="noprint" style="display: block; text-align: left; height: 18px; padding-top: 1px; margin-top: 1px; margin-left: 4px;">
                            <a href="http://ladotnet" class="noindent">Intranet </a>
                            <a href="http://wwwsp.dotd.la.gov/StyleGuide">DOTD Style Guides </a>
                            <a href="home.aspx">Intranet Style Guide</a>
                        </span>

                        <div id="QuickButtons" style="display: block; margin-top: 0px;">
                            <div id="QB2"><a href="http://ladotd:3002/phon0100" title="Phone Directory" class="linkstyle_quick"></a></div>
                            <div id="QB1"><a href="https://radar.weather.gov/radar.php?rid=lix&amp;product=N0R&amp;overlay=11101111&amp;loop=no" title="Weather" class="linkstyle_quick"></a></div>
                            <div id="QB3"><a href="http://apps2/departmentwide/leaveslip" title="ISIS / HR Leave Slip" class="linkstyle_quick"></a></div>

                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div style="height:100%; vertical-align: top;">


                    <div class="body-content">
                        @RenderBody()
                    </div>


                </div>
            </div>
        </div>

        @*<div id="Div3" class="grey_spacer"></div>*@
        <div id="BaseBar">
            <div id="lblDateTime" class="txt_format_1 float_left" runat="server">&nbsp;&nbsp;@DateTime.Now.ToString("dddd,MMMM dd,yyyy")</div>
            <div><a href="mailto:OTSSupport@la.gov?cc=jerry.mason@la.gov&amp;subject=Problem on an intranet page&amp;body=Please describe in detail the issue and submit this email." class="linkstyle_green">Report Page</a></div>
            <div><a href="http://ladotnet/indotd_sitemap.asp" class="linkstyle_green" title="Sitemap">Site Map</a></div>
            <div><a href="http://ladotd:3002/phon0100" class="linkstyle_green" title="Phone / Search Engine">Phone / Search</a></div>
        </div>

    </div>





    @*@Scripts.Render("~/bundles/custom-datatable")*@


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
